<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.preferrd.menu.main.dao.UserDao">
    <select id="getNameById" parameterType="java.lang.String"
            resultType="com.preferrd.menu.main.model.User">
		SELECT * FROM test WHERE id = #{id}
	</select>
    <insert id="addUser" parameterType="com.preferrd.menu.main.model.User">
		INSERT INTO test VALUES (#{id}, #{name})
	</insert>

</mapper>
